use VOIP;
CREATE TABLE `CDR` (
  `StartTime` datetime,
  `CallDuration` datetime,
  `SourceIP` varchar(16),
  `SourceQ931Port` mediumint(9),
  `DestIP` varchar(16),
  `SourceCustID` varchar(32),
  `CalledNumAtDest` varchar(64),
  `CalledNumFromSrc` varchar(64),
  `CallType` varchar(4),
  `DisconnectCode` char(2),
  `CallError1` mediumint(9),
  `CallErrorDesc1` varchar(96),
  `ANI` varchar(64),
  `SequenceNum` int(11),
  `CallID` varchar(128),
  `HoldTime` mediumint(9),
  `SourceRegid` varchar(64),
  `SourcePort` smallint(6),
  `DestRegid` varchar(64),
  `DestPort` smallint(6),
  `ISDNCause` mediumint(9),
  `CalledNumAfterSrcCP` varchar(64),
  `CallError2` mediumint(9),
  `CallErrorDesc2` varchar(96),
  `CallErrorEvent` varchar(96),
  `NewANI` varchar(64),
  `Duration` int(11),
  `IncomingCallId` varchar(128),
  `Protocol` varchar(8),
  `CDRType` varchar(8),
  `HuntingAttempts` smallint(6),
  `TrunkGroup` varchar(64),
  `PDD` int(11),
  `H323RASError` smallint(6),
  `H323H225Error` smallint(6),
  `SIPResponseCode` smallint(6)
);
