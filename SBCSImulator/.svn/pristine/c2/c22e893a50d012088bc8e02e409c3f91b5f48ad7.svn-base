<DB>
  <EV name="1">
    <HOST> All </HOST>
    <DESC> "Action" is taken when "Event" string match is found in the "Type" files on "Host". </DESC>
    <STATUS>ENABLED</STATUS>
    <EQUALS> GENBAND iServer started </EQUALS>
    <ACTION NAME="NONE" />
    <QUAL> logdesc </QUAL>
    <EVENT> iserver restart </EVENT>
  </EV>
  <EV name="2">
    <DESC> "Action" is taken when "Event" string match is found in the "Type" files on "Host". </DESC>
    <HOST> All </HOST>
    <RESET> yes </RESET>
    <STATUS>DISABLED</STATUS>
    <EQUALS> Array full </EQUALS>
    <ACTION SUBJ="jdfhfh" NAME="MAIL" TO="sdfjsdf" BCC="rwtytetye" CC="asdfsdfsdf" />
    <QUAL> logdesc </QUAL>
    <EVENT> array overflow </EVENT>
  </EV>
  <EV name="3">
    <HOST> All </HOST>
    <DESC> "Action" is taken when "key" condition is found in the "Type" files on "Host" for "Duration". </DESC>
    <LT> 75 </LT>
    <STATUS>DISABLED</STATUS>
    <TIME_AVERAGE> 5 </TIME_AVERAGE>
    <ACTION FILE="aewriwerjkdf" NAME="LOG" />
    <QUAL> cdrdesc </QUAL>
    <KEY> asr </KEY>
    <EVENT> asr </EVENT>
  </EV>
  <EV name="4">
    <DESC> "Action" is taken when "key" condition is found in the "Type" files on "Host" for "Duration". </DESC>
    <HOST> All </HOST>
    <LT> 75 </LT>
    <STATUS>ENABLED</STATUS>
    <TIME_AVERAGE> 5 </TIME_AVERAGE>
    <ACTION FILE="adsfasdf" NAME="LOG" />
    <QUAL> cdrdesc </QUAL>
    <KEY> local-disconnect </KEY>
    <EVENT> local disconnect </EVENT>
  </EV>
  <EV name="5">
    <DESC> "Action" is taken when "Event" string match is found in the "Type" files on "Host". </DESC>
    <HOST> All </HOST>
    <STATUS> ENABLED </STATUS>
    <EQUALS> GENBAND iServer started </EQUALS>
    <ACTION NAME="NONE" />
    <QUAL> logdesc </QUAL>
    <EVENT> iserver restart </EVENT>
  </EV>
</DB>
